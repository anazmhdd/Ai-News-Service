{
  "version": 2,
  "builds": [
    {
      "src": "sever.js",
      "use": "@vercel/node",
      "config": {
        "maxDuration": 30
      }
    }
  ],
  "routes": [
    { "src": "/news", "dest": "/news" },
    { "src": "/breaking", "dest": "/breaking" },
    { "src": "/news/save", "dest": "/news/save" },
    { "src": "/news/by-title", "dest": "/news/by-title" },
    { "src": "/news/delete", "dest": "/news/delete" },
    { "src": "/.*", "dest": "/" }
  ]
}
