{
  "version": 2,
  "builds": [
    {
      "src": "server.js",
      "use": "@vercel/node",
      "config": {
        "maxDuration": 60
      }
    }
  ],
  "routes": [
    { "src": "/news", "dest": "/server.js" },
    { "src": "/breaking", "dest": "/server.js" },
    { "src": "/news/save", "dest": "/server.js" },
    { "src": "/news/by-title", "dest": "/server.js" },
    { "src": "/news/delete", "dest": "/server.js" },
    { "src": "/.*", "dest": "/server.js" }
  ]
}
